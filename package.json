{
  "name": "react-mobile-cropper",
  "version": "0.10.0",
  "description": "The react mobile cropper inspired by Android mobile croppers",
  "author": "Norserium",
  "license": "MIT",
  "repository": "Norserium/react-mobile-cropper",
  "main": "dist/index.cjs.js",
  "unpkg": "dist/index.global.js",
  "jsdelivr": "dist/index.global.js",
  "module": "dist/index.esm-bundler.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "react",
    "cropper",
    "mobile",
    "library",
    "advanced",
    "lightweight",
    "customizable",
    "flex"
  ],
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "build": "npm run build:clean && npm run build:library && npm run build:styles",
    "build:clean": "node scripts/clean.js",
    "build:library": "rollup -c",
    "build:styles": "node scripts/styles.js",
    "start": "rollup -c -w",
    "prepare": "yarn run build",
    "predeploy": "cd example && yarn build",
    "deploy": "gh-pages -d example/build"
  },
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0 || ^17.0.0 || ^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^3.0.0",
    "@rollup/plugin-typescript": "^8.2.5",
    "@types/debounce": "^1.2.0",
    "@types/node": "^14.14.7",
    "@types/react": "^16.9.56",
    "@typescript-eslint/eslint-plugin": "^1.6.0",
    "@typescript-eslint/parser": "^1.6.0",
    "@wessberg/rollup-plugin-ts": "^1.3.14",
    "cross-env": "^5.1.4",
    "cssnano": "^5.0.8",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-standard": "^3.1.0",
    "fs-extra": "9.1.0",
    "gh-pages": "^1.2.0",
    "glob": "^8.0.3",
    "prettier": "^2.4.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-scripts": "^2.1.8",
    "rollup": "^2.78.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-visualizer": "^5.6.0",
    "sass": "^1.54.4",
    "typescript": "^4.0.5"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "classnames": "^2.2.6",
    "react-advanced-cropper": "~0.19.3",
    "tslib": "^2.4.0"
  },
	"sideEffects": ["**/*.css"]
}
